</body>
<script src="/javascripts/jquery-3.4.1.js" charset="utf-8"></script>
<script type="text/javascript">

$('#email').blur(function(){
  var data={email:$('#email').val()};
  if(data.email != ''){
  $.ajax({url:'/duemail',data:data,type:'POST',
success:function (result) {
  if(result.status){
    alert('Duplicate');
    $('#email').val('');
    $('#email').focus();



  }
  }


})
}
})
$('#sub').click(function () {
  var datap={pwd:$('#pwd').val()};
  if(datap.pwd !=''){
  $.ajax({url:'/dupwd',data:datap,type:'POST',
success:function (result) {
  if(result.status){
    if($('#email').val() == '' || $('#name').val() ==''){
      alert('Please enter other field');}
      else {
          frm.submit();

      }
    }

  else {

    alert('Password Invalid');
    $('#pwd').val('');
    $('#pwd').focus();
  }

}

})
}
})


</script>
